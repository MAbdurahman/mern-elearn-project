{
  "name": "mern-elearn-project",
  "version": "1.0.0",
  "description": "MongoDB, ExpressJS, ReactJS, and NodeJS application",
  "main": "./backend/server.js",
  "repository": "https://github.com/MAbdurahman/mern-elearn-project.git",
  "author": "Mahdi Abdurrahman <mdbdrrhm@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "SET NODE_ENV=development& nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "SET NODE_ENV=production& node backend/server",
    "seed:data": "node ./backend/utils/seederUtils.js"
  },
  "dependencies": {
    "@clerk/express": "^1.3.24",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.4",
    "multer": "^2.0.2",
    "stripe": "^17.5.0",
    "svix": "^1.42.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10"
  },
  "nodemonConfig": {
    "delay": 2000
  }
}